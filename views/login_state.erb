
	<div class="main">
    <h2 class="form-signin-heading">ログイン状態一覧</h2>
    <form action="delete_id" method="get">
      <table class="table table-striped table-bordered">
      <thead>
          <tr><th>名前</th><th>ログイン中</th><th>最後にログインした時間</th><th>合計ログイン時間</th></tr>
      </thead>
      <tbody>
          <% @table.each {|i| %>
          <%= "<tr><td>#{h i.name}</td><td>#{i.login}</td><td>#{i.last_login.new_offset(Rational(9, 24))}</td><td>#{i.login_minute}</td></tr>" %>
          <% } %>
      </tbody>
      </table>
    </form>
  </div>

